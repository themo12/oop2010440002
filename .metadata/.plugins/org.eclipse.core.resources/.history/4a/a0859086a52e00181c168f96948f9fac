
public class Twentythree {

	public static void main(String[] args) {
		System.out.println("23번 문제");
		// 23. ***** *
		//     *** ***
		//     * *****
		//     를 출력하시오(반복문 써서!)
		
		int i;
		for (i=1; i<=7; i++) {
			if (i != 6)
				System.out.print("*");
			else
				System.out.print(" ");
		}
			
		System.out.println("==============프로그램 종료==============");
	}

}
